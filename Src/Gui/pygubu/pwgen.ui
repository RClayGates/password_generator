<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="tk.Toplevel" id="toplevel1">
    <property name="height">200</property>
    <property name="width">200</property>
    <child>
      <object class="ttk.Frame" id="frame1">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">0</property>
          <property name="sticky">nsew</property>
        </layout>
        <child>
          <object class="ttk.Entry" id="entry1">
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">0 6</property>
              <property name="pady">6</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="button1">
            <property name="text" translatable="yes">Generate</property>
            <layout manager="grid">
              <property name="column">2</property>
              <property name="padx">6</property>
              <property name="pady">6</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="label1">
            <property name="text" translatable="yes">Primary Password:</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="padx">6 0</property>
              <property name="pady">6</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="entry2">
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">0 6</property>
              <property name="pady">6</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="button2">
            <property name="text" translatable="yes">Add</property>
            <layout manager="grid">
              <property name="column">2</property>
              <property name="padx">6</property>
              <property name="pady">6</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="label2">
            <property name="text" translatable="yes">Account Website URL:</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="padx">6 0</property>
              <property name="pady">6</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Frame" id="frame2">
            <property name="height">200</property>
            <property name="width">200</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="columnspan">3</property>
              <property name="padx">6</property>
              <property name="pady">6 0</property>
              <property name="row">4</property>
              <property name="sticky">nsew</property>
            </layout>
            <containerlayout manager="grid">
              <property type="col" id="all" name="weight">1</property>
              <property type="row" id="all" name="weight">1</property>
            </containerlayout>
            <child>
              <object class="ttk.Label" id="label3">
                <property name="text" translatable="yes">Select an entry from the table then click on of the below buttons:</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="columnspan">3</property>
                  <property name="padx">6</property>
                  <property name="pady">6</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="button3">
                <property name="text" translatable="yes">Regenerate password</property>
                <layout manager="grid">
                  <property name="column">1</property>
                  <property name="padx">6</property>
                  <property name="pady">6 0</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="button4">
                <property name="text" translatable="yes">Delete Account Entry</property>
                <layout manager="grid">
                  <property name="column">2</property>
                  <property name="padx">6</property>
                  <property name="pady">6 0</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="button5">
                <property name="text" translatable="yes">Copy password to clipboard</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="padx">6</property>
                  <property name="pady">6 0</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="frame3">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">1</property>
          <property name="sticky">nsew</property>
        </layout>
        <containerlayout manager="grid">
          <property type="col" id="0" name="weight">1</property>
          <property type="row" id="0" name="weight">1</property>
        </containerlayout>
        <child>
          <object class="ttk.Treeview" id="treeview1">
            <property name="selectmode">extended</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="padx">6 0</property>
              <property name="pady">6 0</property>
              <property name="row">0</property>
              <property name="sticky">nsew</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Scrollbar" id="scrollbar1">
            <property name="orient">horizontal</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="padx">6 0</property>
              <property name="pady">0 6</property>
              <property name="row">1</property>
              <property name="sticky">new</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Scrollbar" id="scrollbar2">
            <property name="orient">vertical</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">0 6</property>
              <property name="pady">6 0</property>
              <property name="row">0</property>
              <property name="sticky">nsw</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
